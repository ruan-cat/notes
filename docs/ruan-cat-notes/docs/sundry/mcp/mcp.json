{
	"mcpServers": {
		"context7": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@upstash/context7-mcp"
			]
		},
		"fetch-mcp": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@tokenizin/mcp-npx-fetch"
			],
			"autoApprove": [
				"fetch_markdown"
			]
		},
		"chrome-devtools": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"chrome-devtools-mcp@latest"
			]
		},
		"gong-rzhe-office-word-mcp-server": {
			"type": "http",
			"url": "https://server.smithery.ai/@GongRzhe/Office-Word-MCP-Server/mcp"
		}
	}
}