{
	"mcpServers": {
		"context7": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@upstash/context7-mcp"
			]
		},
		"chrome-devtools": {
			"type": "stdio",
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"chrome-devtools-mcp@latest"
			],
			"env": {}
		},
		"office-word-mcp-server": {
			"command": "uvx",
			"args": [
				"--from",
				"office-word-mcp-server",
				"word_mcp_server"
			]
		},
		"gemini": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/GuDaStudio/geminimcp.git",
				"geminimcp"
			]
		}, 
		"github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer 自己设置的github token"
      }
    }
	}
}